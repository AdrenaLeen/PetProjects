@model ProductsListViewModel
@foreach (Product p in Model.Products)
{
    <partial name="ProductSummary" model="p" />
}
<div page-model="@Model.PagingInfo" page-action="List" page-classes-enabled="true" page-class="btn" page-class-normal="btn-outline-secondary" page-class-selected="btn-primary" page-url-category="@Model.CurrentCategory" class="btn-group float-right"></div>